
.service {
		display: flex;
		height: 70rpx;
		background: #f7f7f7;
		/* 垂直居中 */
		align-items: center;
		justify-content: space-between;
}
.serviceItem {
		font-size: 24rpx;
		color: #a3a3a3;
		line-height: 70rpx;
		display: flex;
		align-items: center;
		margin-left: 20rpx;
		margin-right: 20rpx;
}
.serviceItem image{
		width: 35rpx;
		height: 35rpx;
		margin-right: 10rpx;
}
.shopNav {
		height: 225rpx;
		display: flex;
		justify-content: center;
		background: white;
}
.shopNavItem{
		margin-left: 10rpx;
		margin-right: 10rpx;
}
.shopNavItem image{
		width: 100rpx;
		height: 100rpx;
		margin-top: 30rpx;
}
.shopNavItem text{
		display: block;
		font-size: 24rpx;
		color: #000000;
		line-height: 100rpx;
}
